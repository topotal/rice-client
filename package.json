    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "titanium": "^5.0.6",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  },
